        "jest": "^26.0.1",
        "prettier": "2.0.5"
    },
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "gatsby develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest",
        "jest:w": "jest --watch",
        "sass": "sass --watch src/styles/scss/index.scss:src/styles/css/index.css"
